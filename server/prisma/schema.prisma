generator cliente {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = "mysql://root:password@localhost:3306/leads"
}

model lead {
    id          Int      @id @default(autoincrement())
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
    name        String
    email       String
    phone       String
    status      String?  @default("novo")
    description String
}
